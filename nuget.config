<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <config>
        <add key="dependencyVersion" value="Lowest" />
        <add key="globalPackagesFolder" value="./packages" />
    </config>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <add key="cluedin" value="https://pkgs.dev.azure.com/CluedIn-io/Public/_packaging/Public/nuget/v3/index.json" />
        <add key="chad" value="https://nuget.pkg.github.com/chadnaparite/index.json" />
    </packageSources>
    <packageSourceCredentials>
      <chad>
        <add key="Username" value="chadnaparite" />
        <add key="ClearTextPassword" value="{TOKEN}" />
      </chad>
    </packageSourceCredentials>
</configuration>
